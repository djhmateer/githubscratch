@model List<MiniProfilerTest.Controllers.Contact>

<h1>Contact</h1>

<table class="table">
    @foreach (var item in Model) {
        <tr>
            <td>@item.Id</td>
            <td>@item.Title</td>
            <td>@item.FirstName</td>
            <td>@item.LastName</td>
            <td>@item.Email</td>
            <td>@item.Company</td>
        </tr>
    }
</table>